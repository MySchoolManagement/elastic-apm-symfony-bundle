{
    "name": "myschoolmanagement/elastic-apm-symfony-bundle",
    "type": "symfony-bundle",
    "description": "Integrate Elastic APM into Symfony",
    "keywords": ["metric", "performance", "monitoring"],
    "homepage": "https://github.com/MySchoolManagement/elastic-apm-symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1|^8.0",
        "symfony/config": "^3.4|^4.0|^5.0|^6.0",
        "symfony/console": "^3.4|^4.0|^5.0|^6.0",
        "symfony/dependency-injection": "^3.4|^4.0|^5.0|^6.0",
        "symfony/event-dispatcher": "^3.4|^4.0|^5.0|^6.0",
        "symfony/http-kernel": "^3.4|^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^3.1|^4.0",
        "nyholm/symfony-bundle-test": "^1.6",
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": { "ElasticApmBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "ElasticApmBundle\\Tests\\": "Tests/" }
    }
}
